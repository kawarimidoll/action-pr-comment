{
  "name": "action-pr-comment",
  "version": "1.0.0",
  "private": true,
  "description": "GitHub Action to comment of PR",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "ci": "yarn install --frozen-lockfile",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "preci": "rimraf node_modules",
    "test": "jest",
    "all": "yarn ci && yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kawarimidoll/action-pr-comment.git"
  },
  "keywords": [
    "actions",
    "node"
  ],
  "author": "kawarimidoll",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@octokit/webhooks": "^8.5.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.31",
    "@typescript-eslint/parser": "^4.16.1",
    "@vercel/ncc": "^0.27.0",
    "eslint": "^7.20.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.5",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.0.0",
    "nock": "^13.0.9",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.2"
  }
}
